generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum UploadStatus {
  pending
  completed
  failed
}

enum ProcessingStatus {
  pending
  completed
  failed
}

model Logs {
  logId            Int              @id @default(autoincrement())
  fileName         String
  s3FilePath       String
  serverName       String
  uploadTimestamp  DateTime         @default(now())
  fileSize         Int
  fileType         String
  uploadStatus     UploadStatus
  processingStatus ProcessingStatus
}
